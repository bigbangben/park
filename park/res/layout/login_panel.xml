<?xml version="1.0" encoding="utf-8"?>
<lanyotech.cn.park.layout.LoginPanel xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    nextRes="$layout.register_panel" >

    <lanyotech.cn.park.component.TopBarPanel
        android:id="@+id/topBarPanel"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        topBarRes="$layout.topbar_login"
        android:orientation="vertical" >

        <lanyotech.cn.park.layout.LoginView
            android:id="@+id/loginView"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </lanyotech.cn.park.component.TopBarPanel>

</lanyotech.cn.park.layout.LoginPanel>