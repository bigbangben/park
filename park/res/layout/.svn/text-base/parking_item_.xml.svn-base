<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >

    <RelativeLayout
        android:id="@+id/parking_left_panel"
        android:layout_width="67dp"
        android:layout_height="match_parent"
        android:background="#E3E5DC"
        android:gravity="center_horizontal" >

        <FrameLayout
            android:id="@+id/parking_praise_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:background="#E3E5DC" >

            <ImageView
                android:id="@+id/praise_icon"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:background="#E3E5DC"
                android:padding="2dp"
                android:src="@drawable/praise_press_sel" />

            <TextView
                android:id="@+id/good_click_tv"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:gravity="center_horizontal"
                android:paddingTop="8dp"
                android:text="0"
                android:textColor="#ffffff"
                android:textSize="9sp" />
        </FrameLayout>

        <TextView
            android:id="@+id/parise_count_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@id/parking_praise_button"
            android:layout_below="@id/parking_praise_button"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="4dp"
            android:text="赞一个"
            android:textColor="#ff9600"
            android:textSize="12sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/mid_panel_park"
        android:layout_width="6.5dp"
        android:layout_height="match_parent"
        android:background="#d5d5d5" />

    <LinearLayout
        android:id="@+id/parking_right_bg"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:background="#E3E5DC" >

        <LinearLayout
            android:id="@+id/parking_right"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical" >

                <RelativeLayout
                    android:id="@+id/parking_right_panel_top"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="#F0F0EF" >

                    <TextView
                        android:id="@+id/parking_name"
                        android:layout_width="wrap_content"
                        android:layout_height="43dp"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical"
                        android:text="百汇园停车场"
                        android:textColor="#3269A2"
                        android:textSize="18sp" />

                    <ImageView
                        android:id="@+id/parking_iv_vip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/parking_name"
                        android:layout_marginLeft="4dp"
                        android:layout_marginTop="12dp"
                        android:layout_toRightOf="@+id/parking_name"
                        android:contentDescription="@null"
                        android:src="@drawable/home_list_vip" />

                    <ImageView
                        android:id="@+id/parking_iv_more"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_alignTop="@+id/parking_name"
                        android:layout_marginRight="8dp"
                        android:layout_marginTop="9dp"
                        android:src="@drawable/detailicon_more" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/parking_right_panel_mid"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="#F3F4F3" >

                    <LinearLayout
                        android:id="@+id/tagView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingTop="10dp" >

                        <TextView
                            android:id="@+id/money_charge"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_weight="1"
                            android:text="收费：10元/次"
                            android:textColor="#feab0c"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/parking_distance"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_horizontal"
                            android:text="距离：366m"
                            android:textColor="#51c1cd"
                            android:textSize="15sp" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/parking_tv_dizhi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tagView"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:lines="1"
                        android:paddingBottom="6dp"
                        android:text="地址："
                        android:textColor="#666666"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/parking_tv_location"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tagView"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@id/parking_tv_dizhi"
                        android:lines="1"
                        android:paddingBottom="6dp"
                        android:text="浦东新区世纪大道2000号"
                        android:textColor="#666666"
                        android:textSize="15sp" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#F3F4F3"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:singleLine="true"
                        android:text="攻略："
                        android:textColor="#666666"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/parking_strategy"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:singleLine="false"
                        android:textColor="#666666"
                        android:textSize="15sp" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    android:background="#F3F4F3" />

                <LinearLayout
                    android:id="@+id/parking_bottom_panel"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="#E8ECE8"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:id="@+id/guide_to_parking"
                        style="@style/parking_bottom_panel_style" >

                        <ImageView
                            android:id="@+id/navigation_icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/detailicon_navigation" />

                        <TextView
                            android:id="@+id/navigation_tx"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="导航"
                            android:textColor="#878787"
                            android:textSize="@dimen/parking_detail_text_size" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/share_parking"
                        style="@style/parking_bottom_panel_style" >

                        <ImageView
                            android:id="@+id/share_icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/detailicon_repeat" />

                        <TextView
                            android:id="@+id/share_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="分享"
                            android:textColor="#878787"
                            android:textSize="@dimen/parking_detail_text_size" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/parking_wrong_undo"
                        style="@style/parking_bottom_panel_style" >

                        <ImageView
                            android:id="@+id/parking_wrong_icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/detailicon_wrong" />

                        <TextView
                            android:id="@+id/parking_wrong_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="纠错"
                            android:textColor="#878787"
                            android:textSize="@dimen/parking_detail_text_size" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>